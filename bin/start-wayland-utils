#!/bin/sh
# Start running crucial Wayland utilities.
swaybg -m center -i ~/share/exa-pico3.png -c "#000000" &
waybar &
dunst &
swayidle -w before-sleep swaylock &
kanshi &

gammastep-indicator &

/usr/libexec/ibus-ui-gtk3 --enable-wayland-im --exec-daemon --daemon-args "--xim --panel disable" &

start-mympd
